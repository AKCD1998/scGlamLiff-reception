# Frontend API base (required)
VITE_API_BASE_URL=https://<BACKEND_SERVICE_ONRENDER>

# Backward-compat alias (optional; keep same value as VITE_API_BASE_URL if used)
VITE_API_BASE=

# Set true only when static site intentionally rewrites /api/* to backend.
VITE_ALLOW_SAME_ORIGIN_API=false

# Optional frontend diagnostics
VITE_LOG_API_BASE=

# Optional: hide obvious E2E test rows in UI (`true`/`false`). Default is auto (`true` in PROD).
VITE_HIDE_E2E_RECORDS=

# Optional integration endpoints/keys (set in your own environment; do not commit secrets)
VITE_APPOINTMENTS_GAS_URL=
VITE_APPOINTMENTS_GAS_KEY=
